{
  "Author": "Josh Roman",
  "Name": "macOS Memory",
  "Description": "Monitor macOS memory and swap usage with color-coded progress rings",
  "Version": "1.0.0.0",
  "UUID": "com.joshroman.macos-memory",
  "SDKVersion": 2,
  "Software": {
